# 
# db for file locations 
# note: folders must end with a "/"
# location in repo <space> location on system
#

xorg/.xinitrc                    ~/.xinitrc
xorg/.Xresources                 ~/.Xresources
xorg/20-intel.conf               /etc/X11/xorg.conf.d/20-intel.conf
xorg/30-touchpad.conf            /etc/X11/xorg.conf.d/30-touchpad.conf
xorg/lock-on-suspend@.service    /etc/systemd/system/lock-on-suspend@.service
xorg/compton.conf                ~/.config/compton.conf
xorg/dunstrc                     ~/.config/dunst/dunstrc
xorg/i3-config                   ~/.config/i3/config
xorg/i3status-config             ~/.config/i3status/config

zsh/.dircolors                   ~/.config/zsh/.dircolors
zsh/.zshenv                      ~/.zshenv
zsh/.zshrc                       ~/.config/zsh/.zshrc

nvim/                            ~/.config/nvim/

applications/                    ~/.local/share/applications/

task/on-exit-hook                ~/.task/hooks/on-exit
task/.taskrc                     ~/.taskrc

systemd/fetch-next-event.timer   ~/.config/systemd/user/fetch-next-event.timer
systemd/fetch-next-event.service ~/.config/systemd/user/fetch-next-event.service

systemd/lock-on-suspend@.service /etc/systemd/system/lock-on-suspend@.service

modprobe.d/nowatchdog.conf       /etc/modprobe.d/nowatchdog.conf
modprobe.d/powertop.conf         /etc/modprobe.d/powertop.conf

sysctl.d/99-sysctl.conf          /etc/sysctl.d/99-sysctl.conf

http-prompt/config.py            ~/.config/http-prompt/config.py

my-bin/fetch-next-event          ~/.bin/fetch-next-event
my-bin/lock-screen               ~/.bin/lock-screen
my-bin/toggle-redshift           ~/.bin/toggle-redshift
my-bin/toggle-xautolock          ~/.bin/toggle-xautolock

xrandr/													 ~/.config/xrandr/
