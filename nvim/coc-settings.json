{
	"languageserver": {
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		}, 
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": ["~"]
		}
	},
	"coc.preferences.extensionUpdateCheck": "weekly",
	"cSpell.userWords": [
    "Docx",
    "Docxtemplater",
    "Feasability",
    "Firestore",
    "GPIO",
    "Laravel",
    "Neomerx",
    "PGHOST",
    "Randomizer",
    "adminer",
    "busybox",
    "contakt",
    "dreamfactory",
    "entrypoint",
    "fsel",
    "gitlab",
    "graphql",
    "kubernetes",
    "nameserver",
    "nodebuffer",
    "odata",
    "postgres",
    "postgresql",
    "promisify",
    "rapidm",
    "toastr",
    "uuidv",
    "xslx"
  ], 
	"cSpellExt.enableDictionaries": ["german"], 
	"cSpell.language": "en,de"
}
