"lock-screen"
	Mod4 + Escape

"passmenu -i -l 5 -fn 'Bitstream Vera Sans-8' -nf '#88909f' -nb '#2c2e34' -sf '#2c2e34' -sb '#c5cdd9'"
	Mod4 + p

"xbacklight -dec 10"
	XF86MonBrightnessDown

"xbacklight -inc 10"
	XF86MonBrightnessUp

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
	XF86AudioMute

"pactl set-sink-volume @DEFAULT_SINK@ -5%"
	XF86AudioLowerVolume

"pactl set-sink-volume @DEFAULT_SINK@ +5%"
	XF86AudioRaiseVolume

"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
	XF86AudioMicMute

"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause"
	Mod4 + XF86AudioMute

"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Previous"
	Mod4 + XF86AudioLowerVolume

"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Next"
	Mod4 + XF86AudioRaiseVolume

"scrot -s -e 'xclip -sel cliclipboard -t "image/png" -i $f && mkdir -p ~/screenshots && mv $f ~/screenshots/$f'"
	Mod4 + c

"dunstctl close"
	Control+Alt + space
